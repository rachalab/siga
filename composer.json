{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/address": "^2.0",
        "drupal/auto_username": "^1.6",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/calendar_view": "^2.1",
        "drupal/conditional_fields": "4.x-dev@dev",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/core-project-message": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/danse": "^2.3",
        "drupal/date_filter": "^1.0",
        "drupal/epp": "^1.7",
        "drupal/field_group": "^3.6",
        "drupal/field_permissions": "^1.4",
        "drupal/gantt": "^1.0",
        "drupal/geocoder": "^4.25",
        "drupal/geofield": "^1.62",
        "drupal/gin": "^4.0",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/htmlmail": "^4.0@beta",
        "drupal/mail_login": "^4.0",
        "drupal/markup": "^2.0",
        "drupal/maxlength": "^3.1",
        "drupal/message": "^1.6",
        "drupal/message_notify": "^1.4",
        "drupal/message_ui": "^1.3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/optional_end_date": "^1.4",
        "drupal/paragraphs": "^1.18",
        "drupal/pf_email": "^2.3",
        "drupal/pluginformalter": "^1.7",
        "drupal/prepopulate": "^2.5",
        "drupal/push_framework": "^2.3",
        "drupal/readonly_field_widget": "^1.6",
        "drupal/realname": "^2.0",
        "drupal/redirect_after_login": "^3.0",
        "drupal/registration_role": "^2.0",
        "drupal/rules": "^4.0",
        "drupal/smtp": "^1.4",
        "drupal/triples_field": "^1.0",
        "drupal/user_registrationpassword": "^2.0",
        "drupal/views_autocomplete_filters": "^2.0",
        "drupal/views_data_export": "^1.5",
        "drupal/views_display_switch": "^1.1",
        "drupal/views_kanban": "^1.0",
        "drupal/workflow": "1.x-dev@dev",
        "drupal/yearonly": "^9.1",
        "drush/drush": "^12.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
